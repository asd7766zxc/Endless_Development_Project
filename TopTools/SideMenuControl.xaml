        <Button Height="30" Width="30"  FontSize="25" Style="{StaticResource RegularButtonStyle}" HorizontalAlignment="Right" VerticalAlignment="Top" Content="&#xf0c9;" FontFamily="{StaticResource FontAwesome}" />
       
    </Grid>
